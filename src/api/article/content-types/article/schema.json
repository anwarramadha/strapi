{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subtitle": {
      "type": "text"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "stacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::stack.stack"
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    }
  }
}
